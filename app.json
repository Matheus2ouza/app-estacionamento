{
  "expo": {
    "name": "Le√£o Estacionamento",
    "slug": "leao-estacionamento",
    "version": "1.1.1",
    "orientation": "portrait",
    "icon": "./src/assets/images/splash-icon-white.png",
    "scheme": "leao-estacionamento",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/images/splash-icon-blue.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon-white.png",
        "backgroundColor": "#003153"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.leaoestacionamento",
      "permissions": [
        "BLUETOOTH",
        "BLUETOOTH_ADMIN",
        "BLUETOOTH_CONNECT",
        "BLUETOOTH_SCAN",
        "ACCESS_FINE_LOCATION",
        "INTERNET",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "BLUETOOTH",
        "BLUETOOTH_ADMIN",
        "BLUETOOTH_CONNECT",
        "BLUETOOTH_SCAN",
        "ACCESS_FINE_LOCATION",
        "INTERNET",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/splash-icon-blue.png",
          "imageWidth": 200,
          "resizeMode": "contain"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "photosPermission": "O $(PRODUCT_NAME) precisa acessar sua galeria para anexar comprovantes",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      "expo-font",
      "expo-web-browser",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "443cb2b5-54f8-4fee-b403-ec7bad850c7d"
      }
    },
    "owner": "matheusfurtado",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/443cb2b5-54f8-4fee-b403-ec7bad850c7d"
    }
  }
}

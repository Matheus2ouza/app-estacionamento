{
  "expo": {
    "name": "Le√£o Estacionamento",
    "slug": "leao-estacionamento",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/splash-icon-black.png",
    "scheme": "leaoestacionamento",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "cli": {
      "appVersionSource": "project"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon-white.png",
        "backgroundColor": "#003153",
        "monochromeImage": "./src/assets/images/adaptive-icon-black.png"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.leaoestacionamento",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.MANAGE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-notifications",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/splash-icon-white.png",
          "imageWidth": 250,
          "resizeMode": "contain",
          "backgroundColor": "#003153"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      "expo-font",
      "expo-web-browser",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "443cb2b5-54f8-4fee-b403-ec7bad850c7d"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/443cb2b5-54f8-4fee-b403-ec7bad850c7d"
    }
  }
}
